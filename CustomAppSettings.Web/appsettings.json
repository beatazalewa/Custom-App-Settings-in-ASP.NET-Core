{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "PasswordValidator": {
    "Pattern": "^(?=(.*\\d){1})(.*\\S)(?=.*[a-zA-Z\\S])[0-9a-zA-Z\\S]{8,}",
    "PasswordString": "Type your password here",
    "PasswordLength": 10
  }
}

